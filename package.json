{
  "name": "a-phaser-game",
  "description": "A game made with brunch-phaser-typescript",
  "private": true,
  "author": "",
  "version": "0.0.1",
  "repository": "",
  "scripts": {
    "electron:compile": "tsc --outFile public/main.js app/process/main.ts",
    "electron:run": "electron public/main.js",
    "electron": "npm run electron:compile; npm run electron:run",
    "start": "brunch watch",
    "build": "brunch build --production",
    "test": "tsc --noEmit"
  },
  "dependencies": {
    "electron": "^9.1.1",
    "phaser": "^3.17.0"
  },
  "keywords": [
    "brunch",
    "electron",
    "typescript",
    "phaser3"
  ],
  "devDependencies": {
    "auto-reload-brunch": "^2",
    "brunch": "^2.10.17",
    "clean-css-brunch": "^2",
    "css-brunch": "^2",
    "javascript-brunch": "^2",
    "phaser3-docs": "git+https://github.com/photonstorm/phaser3-docs.git",
    "typescript-brunch": "github:niku/typescript-brunch",
    "uglify-js-brunch": "^2"
  }
}